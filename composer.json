{
    "name": "rnd-cosoft/slm-queue-rabbitmq",
    "description": "Zend Framework module that integrates RabbitMQ as queuing system",
    "license": "MIT",
    "type": "zf-module",
    "keywords": [
        "zf3",
        "queue",
        "job",
        "rabbitmq"
    ],
    "homepage": "https://github.com/rnd-cosoft/slm-queue-rabbitmq",
    "config": {
        "process-timeout": 900,
        "bin-dir": "bin/"
    },
    "require": {
        "php": "^7.1",
        "zendframework/zend-console": "^2.7",
        "zendframework/zend-modulemanager": "^2.8",
        "zendframework/zend-stdlib": "^2.5 || ^3.2",
        "zendframework/zend-eventmanager": "^2.5 || ^3.2",
        "zendframework/zend-servicemanager": "^2.5 || ^3.3",
        "zendframework/zend-mvc": "^2.5 || ^3.1",
        "container-interop/container-interop": "^1.2",
        "php-amqplib/php-amqplib": "^2.7",
        "slm/queue": "^1.0",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "SlmQueueRabbitMq\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SlmQueueRabbitMqTest\\": "test"
        }
    }
}
